{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "esnext"
    ],
    "types": [
      "react",
      "react-native",
      "react-redux",
      "redux"
    ]
  },
  "include": [
    "src"
  ]
}